    for (var i = 42; --i &gt;= 0;) {
      alert("Hello " + String(world));
    }
  }
&lt;/script&gt;
&lt;style&gt;
  p { color: pink }
  b { color: blue }
  u { color: "umber" }
&lt;/style&gt;
</pre>
